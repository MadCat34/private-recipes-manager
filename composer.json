{
    "name": "madcat34/private-recipes-manager",
    "type": "project",
    "license": "AGPL-3.0-only",
    "description": "Fork of symfony-tools/recipes-checker, to manage recipes for private bundles",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com",
            "role": "Original author"
        },
        {
            "name": "Nicolas Grekas",
            "email": "p@tchwork.com",
            "role": "Original author"
        },
        {
            "name": "MadCat34",
            "email": "MadCat34@users.noreply.github.com",
            "role": "Fork maintainer"
        }
    ],
    "homepage": "https://github.com/MadCat34/private-recipes-manager",
    "support": {
        "source": "https://github.com/MadCat34/private-recipes-manager",
        "issues": "https://github.com/MadCat34/private-recipes-manager/issues"
    },
    "require": {
        "php": ">=8",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "symfony/console": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/process": "^5.4",
        "symfony/runtime": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
